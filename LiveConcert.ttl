#===== Songs ==========

:hello a :Song ;
	:songName "Hello" .

:bye a :Song ;
	:songName "Good bye?" .

:how_are_you a :Song ;
	:songName "How are you?" .

:fine_thank_you a :Song ;
	:songName "Fine, thank you!" .

:later_dude a :Song ;
	:songName "Later, dude!" .

:hello_again a :Song ;
	:songName "Hello Again!" .

:balls a :Song ;
	:songName "Balls!" .

#======================

#===== Repertoires ====

:rep1 a :Repertoire ;
	:repertoireSongs :hello, :bye, :fine_thank_you .

:rep2 a :Repertoire ;
	:repertoireSongs :how_are_you, :fine_thank_you .

:rep3 a :Repertoire ;
	:repertoireSongs :later_dude, :hello_again .

:rep4 a :Repertoire ;
	:repertoireSongs :bye, :fine_thank_you .

:rep5 a :Repertoire ;
	:repertoireSongs :how_are_you, :hello .

:rep6 a :Repertoire ;
	:repertoireSongs :balls .

