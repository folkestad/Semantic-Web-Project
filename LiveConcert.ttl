#===== Artists ========

:Motorpsycho a :Artist ;
	:artistName "Motorpsycho" ;
	:artistGenre :Rock ;
	:artistRepertoire :rep1, :rep2 .

:Motorhead a :Artist ;
	:artistName "Motorhead" ;
	:artistGenre :Metal ;
	:artistRepertoire :rep2, :rep3 .

:Slayer a :Artist ;
	:artistName "Slayer" ;
	:artistGenre :Metal ;
	:artistRepertoire :rep3, :rep4 .

:Mastodon a :Artist ;
	:artistName "Mastodon" ;
	:artistGenre :Heavy_Metal ;
	:artistRepertoire :rep4, :rep5 .

:TheSword a :Artist ;
	:artistName "The Sword" ;
	:artistGenre :Doom ;
	:artistRepertoire :rep5, :rep1, :rep6 .

